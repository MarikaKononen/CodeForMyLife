<li{?classes} class="{#classes}{.}{@sep} {/sep}{/classes}"{/classes} id="menu_item_{object_id}">
<a href="{url}"{?target} target="{target}"{/target}>{title|s}</a>

{?show_submenu}
	{@eq key=depth value=depth_now}
	{:else}
		{?sub_menu}
			{@set key="depth_now" add=1 /}
			<ul class="sub-menu menu-depth-{depth_now}">
		{/sub_menu}
		{#sub_menu}
			{>"{menuitem_partial}" /}
		{/sub_menu}
		{?sub_menu}
			</ul>
			{@set key="depth_now" subtract=1 /}
		{/sub_menu}
	{/eq}
{/show_submenu}
</li>
